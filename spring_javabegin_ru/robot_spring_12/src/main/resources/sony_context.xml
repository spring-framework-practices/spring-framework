<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"

       default-lazy-init="true"
>

    <bean class="com.trl.impls.sony.SonyHead">
        <property name="color" value="golden"/>
        <qualifier value="sonyGoldenHead"/>
    </bean>

    <bean class="com.trl.impls.sony.SonyHead">
        <property name="color" value="bronze"/>
        <qualifier value="sonyBronzeHead"/>
    </bean>

    <bean id="sonyHand" class="com.trl.impls.sony.SonyHand"/>
    <bean id="sonyLeg" class="com.trl.impls.sony.SonyLeg"/>

</beans>
